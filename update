echo $1 /sys/class/gpio/export
echo $2 /sys/class/gpio/export
echo $3 /sys/class/gpio/export

SW_ONE=$(cat /sys/class/gpio/gpio$1/value)
SW_TWO=$(cat /sys/class/gpio/gpio$2/value)
SW_THREE=$(cat /sys/class/gpio/gpio$3/value)

echo $1 /sys/class/gpio/unexport
echo $2 /sys/class/gpio/unexport
echo $3 /sys/class/gpio/unexport

if [ "$SW_ONE" == "0" ]
then
    if [ "$SW_TWO" == "0" ]
    then
        if [ "$SW_THREE" == "0" ]
        then
            echo "ALL THREE!"
        fi
    fi
fi